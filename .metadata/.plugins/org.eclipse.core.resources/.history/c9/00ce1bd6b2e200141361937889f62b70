package third;

public class Solution {
	//动态规划：
	//f（m,n） = f(m-1,n) + f(m,n-1)
	public int uniquePaths(int m, int n) {
		if( m == 1 || n ==1) return 1;
		   long count = 1;
		   if(m>n){
		       for(int i=1; i<=n-1; i++){
		           count *= (m + i - 1);
		           count /= i;
		       }
		   }else{
		       for(int i=1; i<=m-1; i++){
		           count *= (n + i - 1);
		           count /= i;
		       }
		   }
		   return (int)count;
	}
	
	public static void main(String[] args) {
		Solution solution = new Solution();
		System.out.println(solution.uniquePaths(3, 7));
	}
}

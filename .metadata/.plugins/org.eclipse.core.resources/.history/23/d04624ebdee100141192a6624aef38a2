
public class Solution {
	//结果为排列组合C(m+n-2,m-1)
	public int uniquePaths(int m, int n) {
		//C(n-1, m-1)
		long dividend;
		long divider;
		if (n >= m) {
			dividend = cal(n-1);
			divider = cal(n-m);
		} else {
			dividend = cal(m-1);
			divider = cal(m-n);
			
		}
		
		
		return (int)(dividend/divider);
	}
	
	public long cal(int m, int n) {
		if(n==0 || n==m) {
			return 1l;
		}
		if(n == 1||n==m-1) {
			return m;
		}
		return cal(m-1, n) + cal(m-1, n-1);
	}
	
	public static void main(String[] args) {
		Solution solution = new Solution();
		System.out.println(solution.uniquePaths(36, 7));
	}
}

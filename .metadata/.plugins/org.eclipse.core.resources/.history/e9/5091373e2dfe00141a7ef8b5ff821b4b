public class Solution {
	public boolean searchMatrix(int[][] matrix, int target) {
		int rowSize = matrix.length;
		int columnSize = matrix[0].length;
		
		int low = 0;
		int high = rowSize;
		
		while (low < high) {
			int mid = (low + high) / 2;
			if (matrix[mid][0] <= target) {
				low = mid;
			} else {
				high = mid - 1;
			}
		}
		int rowId = low;
		
		
		return true;
	}
}

import java.util.List;


public class Solution {
	public int maxArea(List<Integer> height) {
		int x = 0;
		int y = height.size() - 1;
		int maxResult = 0;
		
		while (x < y) {
			int temp
		}
	}
	
	public int calcArea(int x, int y, List<Integer> height) {
		return (y-x) * getMin(height.get(x), height.get(y));
	}
	
	public int getMin(int x, int y) {
		return x<=y? x:y;
	}
}

package second;

public class Solution {
	//动态规划：
	//f（m,n） = f(m-1,n) + f(m,n-1)
	public int uniquePathsWithObstacles(int[][] obstacleGrid) {
		int width = obstacleGrid[0].length;
		for (int i=1; i<=m; i++) {
			for (int j=1; j<=n; j++) {
				if (i==1 || j==1) {
					result[i][j] = 1;
				} else {
					result[i][j] = result[i-1][j] + result[i][j-1];
				}
			}
		}
		return result[m][n];
	}
}

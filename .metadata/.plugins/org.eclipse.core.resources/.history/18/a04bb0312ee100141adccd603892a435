
public class Solution {
	public int uniquePaths(int m, int n) {
		//C(n-1, m-1)
		long dividend;
		long divider;
		if (n >= m) {
			dividend = cal(n-1);
			divider = cal(n-m);
		} else {
			dividend = cal(m-1);
			divider = cal(m-n);
			
		}
		
		
		return dividend/divider;
	}
	
	public long cal(int num) {
		if (num == 0) return 1;
		long result = 1l;
		for (int i = 1; i <= num; i++) {
			result = result * i;
		}
		return result;
	}
	
	public static void main(String[] args) {
		Solution solution = new Solution();
		System.out.println(solution.uniquePaths(36, 7));
	}
}

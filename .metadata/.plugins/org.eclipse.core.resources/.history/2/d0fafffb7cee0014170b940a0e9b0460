import java.util.ArrayList;
import java.util.Arrays;
import java.util.List;


public class Solution {
	public List<List<Integer>> combinationSum2(int[] num, int t) {
        Arrays.sort(num);
        List<List<List<Integer>>> dp = new ArrayList<List<List<Integer>>>();
        for (int i = 1; i <= t; i++) {
            List<List<Integer>> newList = new ArrayList<List<Integer>>();
            for (int j = 0; j < num.length && num[j] <= i; j++) {
                if (i == num[j]) newList.add(Arrays.asList(num[j]));
                else for (List<Integer> l : dp.get(i-num[j]-1)) {
                    if (num[j] <= l.get(0)) {
                        List<Integer> cl = new ArrayList<Integer>();
                        cl.add(num[j]); cl.addAll(l);
                        newList.add(cl);
                    }
                }
            }
            dp.add(newList);
        }
        return dp.get(t-1);
    }
	
	public static void main(String[] args) {
		int[] candidates = {10,1,2,7,6,1,5};
		Solution solution = new Solution();
		List<List<Integer>> a = solution.combinationSum2(candidates, 8);
		
		for (List<Integer> l : a) {
			for (Integer w : l) {
				System.out.print(w + " ");
			}
			System.out.println();
		}
	}
}

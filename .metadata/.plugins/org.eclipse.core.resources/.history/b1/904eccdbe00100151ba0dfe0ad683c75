public class Solution {
	public void sortColors(int[] A) {
		int left = 0;
		int right = A.length - 1;
		int i = 0;
		
		while (i < right) {
			if (A[i] == 0) {
				swap(A[left], A[i]);
				i++;
				left++;
			} else if (A[i] == 1) {
				i++;
			} else {
				swap(A[right], A[i]);
				right--;
			}
		}
		
	}
	
	public void swap(int a, int b) {
		a = a^b;
		b = a^b;
		a = a^b;
	}
	
	public static void main(String[] args) {
		
	}
}


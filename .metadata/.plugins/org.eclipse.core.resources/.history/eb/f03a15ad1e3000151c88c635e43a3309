public class Solution {
	public int maxProduct(int[] nums) {
		if (nums == null || nums.length == 0)
			return 0;
		if (nums.length == 1)
			return nums[0];
		
		int maxNum = nums[0];
		int minNum = nums[0];
		
		for (int i=1; i<nums.length; i++) {
			maxNum = Math.max(Math.max(maxNum * nums[i], nums[i]), minNum * nums[i]);
			
		}
	}
}


public class Solution {
	public int maxProfit(int[] prices) {
		int length = prices.length;
		if (length == 0) return 0;
		int profit = 0;
		int minVal = prices[0];
		
		for (int i = 1; i < length; i++) {
			if (prices[i] > minVal) {
				
			} else {
				minVal = prices[i];
			}
		}
		return profit;
	}
}
